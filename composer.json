{
  "name": "zxkiller/oauth2-wechat",
  "type": "library",
  "description": "微信登录认证授权 Wechat login authorization. This package provides Wechat OAuth 2.0 support for the PHP League's OAuth 2.0 Client",
  "authors": [
    {
      "name": "Zhang Xiao",
      "email": "zhangxiaomail@163.com"
    }
  ],
  "keywords": [
    "wechat",
    "weixin",
    "wechat login",
    "weixin login",
    "oauth2 wechat",
    "oauth2 weixin",
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation"
  ],
  "homepage": "https://github.com/zxkiller/oauth2-wechat",
  "license": "MIT",
  "require": {
    "league/oauth2-client": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "suggest": {
    "symfony/var-dumper": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Zxkiller\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zxkiller\\OAuth2\\Client\\Test\\": "tests/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
